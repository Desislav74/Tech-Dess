@model TechDess.Web.ViewModels.ProductTypes.ProductTypeInputModel
@{
    this.ViewData["Title"] = "Add to productType";
}
<form metod="post" asp-area="" asp-controller="ProductTypes" asp-action="Create" class="col-md-6 offset-md-3" enctype="multipart/form-data">
    <h1>@this.ViewData["Title"]</h1>
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <input type="submit" class="btn btn-primary" />
</form>
